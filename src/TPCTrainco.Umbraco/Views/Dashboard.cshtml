<body>
    @Html.Partial("Navigation/Top")
	<div id="root"></div>
    @Html.Partial("Navigation/Bottom")
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"></script>
	@Html.RenderJsHere()
    <script>
        $(function () {
            var windowWidth = $(window).width();
            if (windowWidth > 991) {
                $(".dropdown").hover(
                function () { $(this).addClass('open') },
                function () { $(this).removeClass('open') }
                );
            }

            $('a[href^="#"]:not(.link-geolocation):not([data-toggle="collapse"])').click(function (e) {
                e.preventDefault();
                var hash = $(this).attr('href');
                if ($(hash).length == 0)
                    return false;
                $('html, body').stop(true).animate({
                    scrollTop:
                     $(hash).offset().top - 125
                }, 500);
            });

            $(document).on('click', 'a.link-geolocation', function (e) {
                e.preventDefault();
                if (typeof window.geoLocationLink === 'undefined' || window.geoLocationLink == '')
                    return false;
                window.location.href = window.location.origin + window.geoLocationLink;
            });

            if ($('.alert-visible').length > 0) {
                $('html body').animate({ scrollTop: $('.alert-visible:first').offset().top - 125 });
            }

            if ($('.alert-success').length > 0) {
                setTimeout(function () {
                    $('.alert-success').fadeOut(400, function () { $(this).remove(); });
                }, 5000);
            }
        });
    </script>

</body>
</html>